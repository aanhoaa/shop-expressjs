								{{/if}}
								{{#properties}}
								{{#each this}}
								{{#if this}}
								<span class="ajaxcart__product-meta">{{@key}}: {{this}}</span>
								{{/if}}
								{{/each}}
								{{/properties}}
								
								<span class="ajaxcart__product-meta">{{ vendor }}</span>
								
